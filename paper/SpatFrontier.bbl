    \keyw{CASH FLOW SENSITIVITIES,Consistency,FINANCING CONSTRAINTS,Fixed
  effects,HETEROSCEDASTICITY,Heterogeneity,Investment,MODEL,MODELS,POLICY,Panel
  data,Stochastic frontier models,TAIWAN,TECHNICAL INEFFICIENCY,TIME,stochastic
  frontier model,transformation}
    \strng{namehash}{WHJHCW1}
